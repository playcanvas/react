{
    "$schema": "https://ui.shadcn.com/schema/registry.json",
    "name": "3d blocks",
    "homepage": "https://playcanvas-react.vercel.app/blocks",
    "items": [
      {
        "name": "blocks",
        "title": "3D Blocks",
        "description": "A collection of 3D blocks for PlayCanvas.",
        "type": "registry:style",
        "author": "Mark Lundin",
        "category": ["3d", "blocks", "webgl"],
        "homepage": "https://playcanvas-react.vercel.app/blocks",
        "files": [
            {
                "path": "../lib/.playcanvas-react.mdc",
                "type": "registry:file",
                "target": ".cursor/playcanvas-react.mdc"
            },
            {
                "path": "registry/.blocks.mdc",
                "type": "registry:file",
                "target": ".cursor/blocks.mdc"
            }
        ]
      },
      {
        "name": "splat-viewer",
        "title": "Splat Viewer",
        "description": "A responsive and composable component for displaying Gaussian splats.",
        "type": "registry:block",
        "author": "Mark Lundin",
        "category": ["3d", "viewer", "asset", "webgl"],
        "homepage": "https://playcanvas-react.vercel.app/blocks/splat-viewer",
        "docs": "https://playcanvas-react.vercel.app/blocks/splat-viewer",
        "meta": {
            "version": "0.1.0.beta.1"
        },
        "dependencies": [
            "@playcanvas/react",
            "playcanvas"
        ],
        "files": [
            {
                "path": "registry/splat-viewer.tsx",
                "type": "registry:component"
            }
        ]
      }
    ]
  }